{
  "name": "MICA",
  "version": "4.6.9",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "data-server": "json-server ./server/data/dummy.json",
    "serve": "ng serve",
    "start:dev": "npm-run-all -p -r data-server serve",
    "start:app": "node server/server.js",
    "start": "node server/server.js",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "test:watch": "ng test --code-coverage --source-map --browsers Chrome",
    "test:ubuntu": "ng test --watch=false --code-coverage --source-map --browsers ChromeHeadlessNoSandbox",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "version:up": "./node_modules/.bin/version-cli up",
    "version:tag": "./node_modules/.bin/version-cli confirm --git-tag --git-push",
    "build": "NODE_ENV='production' ng build --prod --aot --base-href /MICA/ && tar cJf deploy/MICA.tar.xz -C dist MICA",
    "build:dev": "ng build --configuration=development --aot --base-href /MICA/ && tar cJf deploy/MICA.tar.xz -C dist MICA",
    "build:pilot": "NODE_ENV='pilot' ng build --configuration=pilot --aot --output-hashing=bundles --base-href /MICA/ && tar cJf deploy/MICA.tar.xz -C dist MICA",
    "build:prod": "NODE_ENV='production' ng build --prod --aot --base-href /MICA/ && tar cJf deploy/MICA.tar.xz -C dist MICA",
    "build:replica": "NODE_ENV='production' ng build --configuration=replica --prod --aot --base-href /MICA/ && tar cJf deploy/MICA.tar.xz -C dist MICA",
    "deploy": "deploy/deploy.sh --prod && rimraf dist",
    "deploy:pilot": "deploy/deploy.sh --pilot && rimraf dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.7",
    "@angular/cdk": "~7.3.7",
    "@angular/cli": "^8.3.5",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@ngrx/effects": "^9.1.0",
    "@ngrx/store": "^9.0.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@sentry/browser": "^5.13.2",
    "@types/angular-material": "^1.1.68",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.4.1",
    "express": "^4.16.4",
    "lodash": "^4.17.15",
    "ng2-dragula": "2.0.0",
    "ngx-page-scroll": "^5.0.0",
    "normalizr": "3.2.3",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.3.0",
    "reselect": "3.0.1",
    "rxjs": "^6.5.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular-redux/store": "^10.0.0",
    "@angular/common": "^8.2.7",
    "@angular/compiler": "^8.2.7",
    "@angular/compiler-cli": "^8.2.7",
    "@angular/core": "^8.2.7",
    "@angular/forms": "^8.2.7",
    "@angular/platform-browser": "^8.2.7",
    "@angular/platform-browser-dynamic": "^8.2.7",
    "@angular/router": "^8.2.7",
    "@auth0/angular-jwt": "^2.0.0",
    "@babel/compat-data": "^7.8.0",
    "@ng-bootstrap/ng-bootstrap": "^5.1.1",
    "@stolen_orange/versioning": "^1.1.21",
    "@types/auth0-lock": "10.16.0",
    "@types/dragula": "2.1.33",
    "@types/jasmine": "2.5.53",
    "@types/lodash": "^4.14.116",
    "@types/node": "8.0.17",
    "@types/source-map": "0.5.2",
    "codelyzer": "^5.0.1",
    "intl": "^1.2.5",
    "jasmine-core": "2.7.0",
    "jasmine-spec-reporter": "4.1.1",
    "json-server": "^0.16.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-nyan-reporter": "0.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "ng2-completer": "^1.6.3",
    "protractor": "^6.0.0",
    "rimraf": "2.6.1",
    "sass-lint": "^1.13.1",
    "ts-node": "5.0.1",
    "tslint": "5.5.0",
    "typescript": "3.5.3",
    "uuid": "^3.3.2",
    "web-animations-js": "^2.3.2",
    "webdriver-manager": "12.0.6",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
